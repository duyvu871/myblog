{
  "name": "boiler-next",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "keywords": [
    "nextjs",
    "starter",
    "template",
    "mantine",
    "tailwind",
    "typescript",
    "react",
    "nextjs",
    "next-auth",
    "prisma"
  ],
  "author": "Bui An Du",
  "license": "MIT",
  "homepage": "https://github.com/duyvu871/boiler-next",
  "repository": {
    "type": "git",
    "url": "https://github.com/duyvu871/boiler-next.git"
  },
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev --turbopack",
    "build": "prisma generate && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "postinstall": "prisma generate",
    "migrate:dev": "dotenv -e .env.local prisma migrate dev",
    "migrate:deploy": "dotenv -e .env.production prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:push:dev": "dotenv -e .env.local prisma db push",
    "db:push:prod": "dotenv -e .env.production prisma db push",
    "db:seed:dev": "dotenv -e .env.local prisma db seed",
    "db:reset:dev": "dotenv -e .env.local prisma migrate reset && prisma db seed",
    "db:studio": "dotenv -e .env.local prisma studio",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@mantine/carousel": "^8.2.5",
    "@mantine/charts": "^8.2.5",
    "@mantine/core": "^8.2.5",
    "@mantine/dates": "^8.2.5",
    "@mantine/dropzone": "^8.2.5",
    "@mantine/hooks": "^8.2.5",
    "@mantine/modals": "^8.2.5",
    "@mantine/notifications": "^8.2.5",
    "@mantine/nprogress": "^8.2.5",
    "@mantine/spotlight": "^8.2.5",
    "@mantine/tiptap": "^8.2.5",
    "@prisma/client": "^6.14.0",
    "@tabler/icons-react": "^3.34.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.85.6",
    "@tanstack/react-query-devtools": "^5.85.6",
    "@tiptap/extension-link": "^3.2.0",
    "@tiptap/pm": "^3.2.0",
    "@tiptap/react": "^3.2.0",
    "@tiptap/starter-kit": "^3.2.0",
    "@uidotdev/usehooks": "^2.4.1",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^10.0.0",
    "dayjs": "^1.11.13",
    "dotenv-cli": "^10.0.0",
    "embla-carousel": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.12",
    "jotai": "^2.13.1",
    "lucide-react": "^0.540.0",
    "next": "15.5.0",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "prisma": "^6.14.0",
    "react": "19.1.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^2.1.0",
    "three": "^0.179.1",
    "usehooks-ts": "^3.1.1",
    "winston": "^3.17.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/winston": "^2.4.4",
    "dotenv": "^17.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
